{
	"name": "komyut-ph",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:node": "USE_NODE_ADAPTER=true vite dev",
		"build": "vite build",
		"preview": "wrangler dev .svelte-kit/cloudflare/_worker.js --port 4173",
		"preview:remote": "wrangler dev .svelte-kit/cloudflare/_worker.js --port 4173 --remote",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"gen": "wrangler types"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^9.39.2",
		"@fontsource/fira-mono": "^5.2.7",
		"@lucide/svelte": "^0.563.1",
		"@neoconfetti/svelte": "^2.2.2",
		"@playwright/test": "^1.58.1",
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/adapter-node": "^5.0.0",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^22",
		"@vitest/browser-playwright": "^4.0.18",
		"bits-ui": "^2.15.5",
		"clsx": "^2.1.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.3.0",
		"playwright": "^1.58.1",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2",
		"wrangler": "^4.63.0"
	},
	"dependencies": {
		"@supabase/ssr": "^0.8.0",
		"@supabase/supabase-js": "^2.97.0",
		"maplibre-gl": "^5.18.0"
	}
}
